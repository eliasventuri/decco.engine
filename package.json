    "name": "decco-engine",
    "version": "1.1.10",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "build": "electron-builder"
    },
    "dependencies": {
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "fluent-ffmpeg": "^2.1.3",
        "torrent-stream": "^1.2.0"
    },
    "devDependencies": {
        "electron": "^28.3.3",
        "electron-builder": "^24.13.3"
    },
    "build": {
        "appId": "com.decco.engine",
        "productName": "Decco Engine",
        "directories": {
            "output": "dist"
        },
        "win": {
            "target": "nsis"
        },
        "protocols": [
            {
                "name": "Decco Protocol",
                "schemes": [
                    "decco"
                ]
            }
        ],
        "nsis": {
            "oneClick": true,
            "allowElevation": true,
            "perMachine": true,
            "deleteAppDataOnUninstall": true,
            "include": "build/installer.nsh"
        }
    }
}